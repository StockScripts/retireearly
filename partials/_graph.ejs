<div class="graph">
  <div class="graph-inner">
    <svg id="graph-svg" class="graph-svg" preserveaspectratio="none">
      <title id="graph-title">Retire early graph</title>
      <polyline class="usage"/>
      <polyline class="profit"/>
      <polyline class="savings"/>
    </svg>
    <div class="yaxis">
      <span class="y-1">100k</span>
      <span class="y-2">500k</span>
      <span class="y-3">900k</span>
    </div>
    <div class="xaxis">
      <span class="x-area start_year"><span></span></span>
      <span class="x-area end_year"><span></span></span>
      <span class="x-area done_year"><span></span></span>
      <span class="x-area over_year"><span></span></span>
    </div>
  </div>
</div>
